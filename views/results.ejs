                100%
              </td>
            </tr>
          </tbody>
        </table>
        <div id="map"></div>
        <script>
          var center = <%- JSON.stringify(summary.question2.points.center.geometry.coordinates) %>;
          var mymap = L.map('map').setView([center[1], center[0]], 4);
          L.tileLayer('https://api.tiles.mapbox.com/v4/{id}/{z}/{x}/{y}.png?access_token=pk.eyJ1IjoibWFwYm94IiwiYSI6ImNpejY4NXVycTA2emYycXBndHRqcmZ3N3gifQ.rJcFIG214AriISLbB6B5aw', {
            maxZoom: 18,
            id: 'mapbox.streets'
          }).addTo(mymap);
          var myGeo = <%- JSON.stringify(summary.question2.points.features) %>
            L.geoJSON(myGeo).addTo(mymap);
          var group = new L.featureGroup(myGeo);</script>
