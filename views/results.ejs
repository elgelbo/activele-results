        <div class="summary-question overall">
          <div class="title">
            <h4>Survey Participation</h4>
          </div>
          <table class="table">
            <thead>
              <tr>
                <th scope="col"></th>
                <th scope="col" class="text-center"># of Responses</th>
                <th scope="col" class="text-center">% of Total</th>
              </tr>
            </thead>
            <tbody>
              <tr>
                <th scope="row">Completed Surveys</th>
                <td class="text-center">
                  <%- JSON.stringify(summary.complete.count) %>
                </td>
                <td class="text-center">
                  <%- summary.complete.percent %> %
                </td>
              </tr>
              <tr>
                <th scope="row">Spanish Surveys</th>
                <td class="text-center">
                  <%- JSON.stringify(summary.spanish.count) %>
                </td>
                <td class="text-center">
                  <%- summary.spanish.percent %> %
                </td>
              </tr>
              <tr>
                <th scope="row">Total Responses</th>
                <td class="text-center">
                  <%- JSON.stringify(summary.total) %>
                </td>
                <td class="text-center">
                  100%
                </td>
              </tr>
            </tbody>
          </table>
        </div>
        <div class="summary-question age">
          <div class="title">
            <h4>Age of Participants</h4>
          </div>
          <h5>The average age of survey participants is
            <%- summary.age %>. </h5>
        </div>
        <div class="summary-question age">
          <div class="title">
            <h4>Opt-In for Future Notifications</h4>
          </div>
          <h5>A total of
            <%- summary.email.total %> participants requested future notifications about ActiveLE. </h5>
          <ol>
            <% summary.email.list.forEach(function(cat, index) { %>
            <li>
              <%= cat %>
            </li>
            <% }); %>
          </ol>
        </div>
      </div>
