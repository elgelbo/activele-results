  <div class="container results">
    <% surveys.forEach(function(survey, index) { %>
    <div class="container result">
      <div class="title">
        <h1>Response #
          <%= index+1 %>
        </h1>
        <h4>Active LE Text Message Survey</h4>
      </div>
      <div class="details">
        <table class="table">
          <thead>
            <tr>
              <th scope="col">Question #</th>
              <th scope="col" class="text-center">Question</th>
              <th scope="col" class="text-center">Answer</th>
            </tr>
          </thead>
          <tbody>
            <% survey.responses.forEach(function(resp, index) { %>
            <tr>
              <th scope="row" class="tbl-1">
                <%= index+1 %>
              </th>
              <td class="text-center tbl-2">
                <%- questions[index].text %>
              </td>
              <td class="text-center tbl-3">
                <%= Object.values(resp) %>
              </td>
            </tr>
            <% }); %>
          </tbody>
        </table>
      </div>
    </div>
