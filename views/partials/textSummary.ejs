<div class="result summary">
  <div class="title">
    <h4>Survey Participation</h4>
  </div>
  <div class="details">
    <table class="table">
      <thead>
        <tr>
          <th scope="col"></th>
          <th scope="col" class="text-center"># of Responses</th>
          <th scope="col" class="text-center">% of Total</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th scope="row" style="width: 25%">
            Completed Surveys
          </th>
          <td class="text-center" style="width: 50%">
            <%- JSON.stringify(summary.complete.count) %>
          </td>
          <td class="text-center" style="width: 25%">
            <%- summary.complete.percent %>%
          </td>
        </tr>
        <tr>
          <th scope="row" style="width: 25%">
            Spanish Surveys
          </th>
          <td class="text-center" style="width: 50%">
            <%- JSON.stringify(summary.spanish.count) %>
          </td>
          <td class="text-center" style="width: 25%">
            <%- summary.spanish.percent %>%
          </td>
        </tr>
        <tr>
          <th scope="row" style="width: 25%">
            Total Surveys
          </th>
          <td class="text-center" style="width: 50%">
            <%- JSON.stringify(summary.total) %>
          </td>
          <td class="text-center" style="width: 25%">
            100%
          </td>
        </tr>
      </tbody>
    </table>
  </div>
</div>
<div class="result age">
  <div class="title">
    <h4>Age of Participants</h4>
  </div>
  <div class="details">
    <h5>
      The average age of survey participants is
      <%- summary.age.average %> and ranged from
      <%- summary.age.low %> years old to
      <%- summary.age.high %> years old.
    </h5>
  </div>
</div>
<div class="result email">
  <div class="title">
    <h4>Opt-In for Future Notifications</h4>
  </div>
  <div class="details">
    <h5>
      A total of
      <%- summary.email.total %> participants requested future notifications about ActiveLE.
    </h5>
    <ol>
      <% summary.email.list.forEach(function(e, index) { %>
      <li>
        <%= e %>
      </li>
      <% }); %>
    </ol>
  </div>
</div>
